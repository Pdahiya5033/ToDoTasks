<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reportintent">

    <application
        android:allowBackup="true"
        android:icon="@android:drawable/presence_online"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ReportIntent">
        <activity
            android:name=".ReportListActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ReportPagerActivity"
            android:label="@string/app_name"
            android:exported="true"
            android:parentActivityName=".ReportListActivity">

        </activity>
    </application>

</manifest>